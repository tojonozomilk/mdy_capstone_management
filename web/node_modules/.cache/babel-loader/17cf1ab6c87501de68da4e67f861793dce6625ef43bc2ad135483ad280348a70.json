{"ast":null,"code":"import UserChat from '@/components/message/UserChat.vue';\nexport default {\n  name: 'ProjectTable',\n  props: {\n    loading: Boolean,\n    options: Object,\n    pageData: Object,\n    dataList: Array,\n    onView: Function,\n    onEdit: Function,\n    onDelete: Function,\n    onFetchData: Function\n  },\n  components: {\n    UserChat\n  },\n  data() {\n    return {\n      projectPageData: this.pageData\n    };\n  },\n  methods: {\n    onFetchProjectData() {\n      const page = this.projectPageData.page;\n      const size = this.projectPageData.size;\n      this.onFetchData(page, size);\n    }\n  }\n};","map":{"version":3,"names":["UserChat","name","props","loading","Boolean","options","Object","pageData","dataList","Array","onView","Function","onEdit","onDelete","onFetchData","components","data","projectPageData","methods","onFetchProjectData","page","size"],"sources":["src/components/table/ProjectTable.vue"],"sourcesContent":["<template>\n    <div>\n        <!-- Project列表 -->\n        <el-table :data=\"projectPageData?.content || dataList\" :row-key=\"row => row.id\" :loading=\"loading\"\n            :height=\"pageData ? '70vh' : '28vh'\">\n            <el-table-column label=\"Project Name\" prop=\"name\" />\n            <el-table-column label=\"Category\" prop=\"category\" />\n            <el-table-column label=\"Creator\">\n                <template slot-scope=\"scope\">\n                    <UserChat :userId=\"scope.row.creatorId\"\n                        :username=\"options.allUserId.find((option) => option.value === scope.row.creatorId)?.label\" />\n                </template>\n            </el-table-column>\n            <el-table-column label=\"Creation Date\" prop=\"createDate\" />\n            <el-table-column label=\"Amend Date\" prop=\"amendDate\" />\n            <el-table-column label=\"Action\">\n                <template slot-scope=\"scope\">\n                    <el-button v-if=\"onView\" type=\"warning\" size=\"small\" @click=\